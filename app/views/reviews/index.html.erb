<div class="album text-muted">
   <div class="container">
      <div class="row">
        <% @reviews.each do |review| %>
        <div class="card">
        <p class="card-text"><%= review.rating %></p>
        <p class="card-text"><%= review.content %></p>
        </div>
        <% end %>
      <div class="row">
  </div>
</div>

<%= link_to "Delete #{@review.rating}",
            restaurant_reviews_path(@review),
            method: :delete,
            data: { confirm: "Are you sure?" } %>